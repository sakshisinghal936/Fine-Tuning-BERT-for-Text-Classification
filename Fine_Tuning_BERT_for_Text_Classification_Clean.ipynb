   "source": [
    "from tqdm import tnrange\n",
    "import gc\n",
    "import numpy as np\n",
    "from sklearn.metrics import accuracy_score, matthews_corrcoef\n",
    "\n",
    "# Lists to track training loss and learning rate per epoch\n",
    "train_loss_set = []\n",
    "learning_rate = []\n",
    "val_accuracy_set = []\n",
    "\n",
    "# Training loop over multiple epochs\n",
    "for epoch in tnrange(1, epochs + 1, desc='Epoch'):\n",
    "    print(\"<\" + \"=\"*22 + f\" Epoch {epoch} \" + \"=\"*22 + \">\")\n",
    "\n",
    "    model.train()              # Set model to training mode\n",
    "    model.zero_grad()          # Clear any previously accumulated gradients\n",
    "    batch_loss = 0             # Initialize cumulative batch loss for the epoch\n",
    "\n",
    "    # Loop through each batch of the training dataloader\n",
    "    for step, batch in enumerate(train_dataloader):\n",
    "        # Move batch data to the same device as the model (GPU or CPU)\n",
    "        b_input_ids, b_input_mask, b_labels = [t.to(device) for t in batch]\n",
    "\n",
    "        # Forward pass: model returns loss and logits\n",
    "        outputs = model(b_input_ids, token_type_ids=None, attention_mask=b_input_mask, labels=b_labels)\n",
    "        loss = outputs[0]\n",
    "\n",
    "        # Backward pass: compute gradients\n",
    "        loss.backward()\n",
    "\n",
    "        # Clip gradients to avoid exploding gradient problem\n",
    "        torch.nn.utils.clip_grad_norm_(model.parameters(), 1.0)\n",
    "\n",
    "        # Optimizer step: update model weights\n",
    "        optimizer.step()\n",
    "\n",
    "        # Scheduler step: adjust the learning rate\n",
    "        scheduler.step()\n",
    "\n",
    "        # Reset gradients before next batch\n",
    "        optimizer.zero_grad()\n",
    "\n",
    "        # Add batch loss to total epoch loss\n",
    "        batch_loss += loss.item()\n",
    "\n",
    "        # Clear memory (useful in Colab or when working with GPU)\n",
    "        del loss, outputs\n",
    "        gc.collect()\n",
    "        torch.cuda.empty_cache()\n",
    "\n",
    "    # Calculate average training loss over all batches\n",
    "    avg_train_loss = batch_loss / len(train_dataloader)\n",
    "\n",
    "    # Print and store the current learning rate\n",
    "    for param_group in optimizer.param_groups:\n",
    "        print(\"\\n\\tCurrent Learning rate:\", param_group['lr'])\n",
    "        learning_rate.append(param_group['lr'])\n",
    "\n",
    "    # Store the average training loss for this epoch\n",
    "    train_loss_set.append(avg_train_loss)\n",
    "    print(f'\\n\\tAverage Training loss: {avg_train_loss}')\n",
    "\n",
    "    # ---------- Evaluation ----------\n",
    "    model.eval()  # Set model to evaluation mode (disables dropout, etc.)\n",
    "\n",
    "    eval_accuracy = 0\n",
    "    eval_mcc_accuracy = 0\n",
    "    nb_eval_steps = 0\n",
    "\n",
    "    # Loop through each batch of the validation dataloader\n",
    "    for batch in validation_dataloader:\n",
    "        b_input_ids, b_input_mask, b_labels = [t.to(device) for t in batch]\n",
    "\n",
    "        with torch.no_grad():  # No gradient computation during evaluation\n",
    "            logits = model(b_input_ids, token_type_ids=None, attention_mask=b_input_mask)[0]\n",
    "\n",
    "        # Move logits and labels to CPU and convert to NumPy for metric calculation\n",
    "        logits = logits.detach().cpu().numpy()\n",
    "        label_ids = b_labels.cpu().numpy()\n",
    "\n",
    "        # Get predicted class labels\n",
    "        pred_flat = np.argmax(logits, axis=1).flatten()\n",
    "        labels_flat = label_ids.flatten()\n",
    "\n",
    "        # Compute accuracy and Matthews Correlation Coefficient for the batch\n",
    "        tmp_eval_accuracy = accuracy_score(labels_flat, pred_flat)\n",
    "        tmp_eval_mcc_accuracy = matthews_corrcoef(labels_flat, pred_flat)\n",
    "\n",
    "        # Accumulate metrics across all batches\n",
    "        eval_accuracy += tmp_eval_accuracy\n",
    "        eval_mcc_accuracy += tmp_eval_mcc_accuracy\n",
    "        nb_eval_steps += 1\n",
    "\n",
    "        # Clean up memory\n",
    "        gc.collect()\n",
    "        torch.cuda.empty_cache()\n",
    "\n",
    "    accuracy = eval_accuracy / nb_eval_steps\n",
    "    val_accuracy_set.append(accuracy)\n",
    "    # Print average evaluation metrics for this epoch\n",
    "    print(f'\\n\\tValidation Accuracy: {eval_accuracy / nb_eval_steps}')\n",
    "    print(f'\\n\\tValidation MCC Accuracy: {eval_mcc_accuracy / nb_eval_steps}')\n"
   ]
